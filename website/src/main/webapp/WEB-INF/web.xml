<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ProjectOperation</servlet-name>
  	<display-name>ProjectOperation</display-name>
  	<description></description>
  	<servlet-class>com.servlets.ProjectOperation</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ProductOperationServlet</servlet-name>
  	<display-name>ProductOperationServlet</display-name>
  	<description></description>
  	<servlet-class>com.servlets.ProductOperationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ProjectOperation</servlet-name>
  	<url-pattern>/ProjectOperation</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ProductOperationServlet</servlet-name>
  	<url-pattern>/ProductOperationServlet</url-pattern>
  </servlet-mapping>
</web-app>
